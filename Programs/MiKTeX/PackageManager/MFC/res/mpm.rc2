#ifdef APSTUDIO_INVOKED
#  error this file is not editable by Microsoft Visual C++
#endif //APSTUDIO_INVOKED

/////////////////////////////////////////////////////////////////////////////
//
// Version
//

VS_VERSION_INFO VERSIONINFO
 FILEVERSION VER_FILEVERSION
 PRODUCTVERSION MIKTEX_VERSION
 FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904e4"
        BEGIN
            VALUE "CompanyName", MIKTEX_COMPANYNAME_STR
            VALUE "FileVersion", VER_FILEVERSION_STR
            VALUE "InternalName", "mpm.exe"
            VALUE "LegalCopyright", MIKTEX_LEGALCOPYRIGHT_STR
            VALUE "OriginalFilename", "mpm.exe"
            VALUE "ProductName", MIKTEX_PRODUCTNAME_STR
            VALUE "ProductVersion", MIKTEX_VERSION_STR
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END
